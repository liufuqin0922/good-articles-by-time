 QSlider::groove:horizontal
{
	border: 0px solid #999999;                             
	width:100px;
	height:1px;                                           
	margin: 1px 0;                                         
	left: 12px; right: 12px;
}
QSlider::groove:horizontal
{
	border: 0px solid #999999;                             
	width:100px;
	height:1px;                                           
	margin: 1px 0;                                         
	left: 12px; right: 12px;
}
QSlider::handle:horizontal
{  
	border-image: url(:/login/login/thumb_style_1.png) 0 38 0 0;
	width:18px;
	height:9Px;
	margin: -5px 0px -6px 0px;
}  

QSlider::handle:horizontal:hover
{  
	border-image: url(:/login/login/thumb_style_1.png) 0 19 0 19;
}  

 QSlider::handle:horizontal:pressed
{  
	border-image: url(:/login/login/thumb_style_1.png) 0 0 0 38;
} 

 QSlider::add-page:horizontal 
{
    background: rgb(202,206,200);
}

 QSlider::sub-page:horizontal
{
    background:rgb(101,187,51);
}

/*********QCheckBox************/
 QCheckBox:focus
{
	border: none;
	outline: none;
}

 QCheckBox
{
	font-size:12px;
	font-family:Î¢ÈíÑÅºÚ,ËÎÌå;
}

 QCheckBox::indicator
{	
	width:15px;
	height:15px;
}

 QCheckBox::indicator:unchecked 
{
	border-image: url(:/setting/setting/style_1.png) 0 105 0 0;
}

 QCheckBox::indicator:unchecked:hover
{
	border-image: url(:/setting/setting/style_1.png) 0 90 0 15;
}

 QCheckBox::indicator:unchecked:pressed
{
	border-image: url(:/setting/setting/style_1.png) 0 75 0 30;
}

 QCheckBox::indicator:unchecked:disabled
{
     border-image: url(:/setting/setting/style_1.png) 0 15 0 90;
}

 QCheckBox::indicator:checked
{ 
	border-image: url(:/setting/setting/style_1.png) 0 60 0 45;
}

 QCheckBox::indicator:checked:hover
{
	border-image: url(:/setting/setting/style_1.png) 0 45 0 60;
}

